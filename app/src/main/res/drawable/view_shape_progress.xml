<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/secondaryProgress">
        <shape
            android:innerRadiusRatio="10"
            android:shape="oval"
            android:thicknessRatio="60"
            android:useLevel="false">

            <stroke
                android:width="5dp"
                android:color="@color/color_transparent_10"
                android:dashGap="0dp"
                android:dashWidth="0dp" />
        </shape>
    </item>

    <item android:id="@+id/shape_drawable">
        <shape android:shape="oval">
            <size
                android:width="56dp"
                android:height="56dp" />

            <stroke
                android:width="5dp"
                android:color="@color/color_transparent_10"
                android:dashGap="0dp"
                android:dashWidth="0dp" />
        </shape>
    </item>
</layer-list>